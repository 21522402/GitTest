<Application x:Class="GitTest.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GitTest"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="./Resource/Fonts/Fonts.xaml"/>
                <ResourceDictionary Source="./Resource/Colors/Colors.xaml"/>
                <ResourceDictionary Source="./Resource/Icons/Icons.xaml"/>
                <ResourceDictionary Source="./Resource/Images/Images.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
